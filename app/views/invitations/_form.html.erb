<%= tao_form invitation, url: invitation_path(invitation.code), remote: true do |f| %>
  <%= f.input :email, label: false, placeholder: true, disabled: true %>
  <%= f.input :name, label: false, placeholder: true %>
  <%= f.input :password, label: false, placeholder: true %>
  <%= f.button :submit, t('.submit') %>
<% end %>
