<!DOCTYPE html>
<html>
  <head>
    <%= render 'layouts/head' %>
  </head>
  <body>
    <%= render 'layouts/operations' %>
    <<%= page_id.dasherize %>-page layout="default" class="page">
      <%= render 'layouts/header' %>
      <div class="page-main">
        <beast-page-content class="page-content">
          <%= yield %>
        </beast-page-content>
      </div>
    </<%= page_id.dasherize %>-page>
  </body>
</html>
