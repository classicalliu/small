<div class="sidelinks">
  <div class="block tags">
    <label><%= t('.taggings') %></label>
    <% Tag.first(20).each do |tag| %>
      <span class="tag"><%= tag.name %></span>
    <% end %>
  </div>
  <div class="block features">
    <label><%= t('.features') %></label>
    <%= link_to t('.archive'), '#' %>
    <%= link_to t('.rss'), '#' %>
    <%= link_to t('.login'), '#' %>
  </div>
</div>
